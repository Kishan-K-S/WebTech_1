        <style>
            @import url('https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&display=swap');

            body,
            html {
                margin: 0;
                padding: 0;
            }

            * {
                box-sizing: border-box
            }

            body {
                font-family: 'Roboto', sans-serif;
                margin-bottom: 400px;
               
            }

            .form {
                width: 100%;
                max-width: 900px;
                margin: 10px auto 30px;
                background: grey;
                padding: 60px;
                border: solid 5px black;
                border-radius: 40px;
            }

            .form h2 {
                font-weight: 500;
                text-align: center;
                margin: 0 auto 15px;
                padding: 0
            }

            .form label {
                display: block;
                margin-bottom: 5px;
                font-size: 16px;
                color: black
            }

            .form input[type=text],
            .form input[type=email],
            .form input[type=number] {
                width: 100%;
                height: 40px;
                outline: none;
                border: solid 1px #e8e8e8;
                padding-left: 10px;
                font-size: 15px;

            }

            .form input[type=text]:focus,
            .form input[type=email]:focus,
            .form input[type=number]:focus {
                border: solid 1px black
            }

            .form input::placeholder,
            .form textarea::placeholder {
                color: silver;
            }

            .form textarea {
                width: 100%;
                height: 80px;
                outline: none;
                border: solid 1px #e8e8e8;
                padding-left: 10px;
                font-size: 15px;
                font-family: 'Roboto', sans-serif;
            }

            .field {
                margin-bottom: 20px;
                /* font-size: 150%; */
            }


            .form textarea:focus {
                border: solid 1px rgba(6, 107, 201, 0.4)
            }

            .form input[type=submit],
            .form input[type=button] {
                padding: 12px 30px;
                outline: none;
                border-radius: 20px;
                background: #06c930;
                color: #fff;
                border: none;
                cursor: pointer;
                font-size: 18px;
                letter-spacing: 1.2px;
            }

            .form input[type=submit]:hover,
            .form input[type=button]:hover {
                background: #d4009f
            }

            .main {
                width: 100%;
                max-width: 900px;
                margin: auto;
            }

            .header {
                width: 100%;
                min-height: 100px;
                background: #157AE4;
                color: #fff;
                padding: 20px;
                margin-top: 15px;
                border-radius: 60px;
            }

            .header h1 {
                font-weight: 500;
            }

            .buttons {
                width: 100%;
                min-height: 50px;
                margin: 15px 0;
            }

            .buttons li {
                list-style: none;
                display: inline-block;
                margin-right: 30px;
            }

            button {
                padding: 12px 30px;
                outline: none;
                border-radius: 2px;
                background: yellowgreen;
                color: #fff;
                border: none;
                cursor: pointer;
                font-size: 16px;
            }

            button:hover {
                opacity: 0.8;
            }
        </style>
    </header>

    <br><br><br><br><br><br><br><br><br><br>
    <div class="card">
        <section class="packages" id="packages">
            <h1 class="heading">
                <span>S</span>
                <span>i</span>
                <span>g</span>
                <span>n</span>
                <span>-</span>
                <span>U</span>
                <span>p</span>
