            <br><br><br><br>
            <div>
                <div class="form">
                    <form method="POST" action="http://localhost:3000/signup">
                        <div>
                            <div class="field" style="margin-right: 400px;">
                                <label class="label">First Name</label>
                                <input type="text" id="first_name" name="first_name"
                                    placeholder="What's your First Name?" style="border-radius: 5px;">
                                <span id="first_name1" style="color:red;"></span>
                            </div>
                            <div class="field" style="margin-left: 400px; margin-top: -86px;">
                                <label class="label">Last Name</label>
                                <input type="text" id="last_name" name="last_name" placeholder="What's your Last Name?"
                                    style="border-radius: 5px;">
                                <span id="last_name1" style="color:red;"></span>
                            </div>
                        </div>
                        <div class="field">
                            <label class="label">Email Id</label>
                            <input type="email" name="email" id="email" placeholder="Type your Email Id"
                                style="border-radius: 5px;">
                            <span id="email1" style="color:red;"></span>
                        </div>
                        <div class="field">
                            <label class="label">Phone Number</label>
                            <input type="text" name="phone" id="phone" placeholder="Type Phone Number"
                                style="border-radius: 5px;">
                            <span id="phone1" style="color:red;"></span>
                        </div>

                        <div class="field">
                            <label class="label">Place of Living</label>
                            <input type="text" name="place" id="place" placeholder="Type Place"
                                style="border-radius: 5px;">
                            <span id="place1" style="color:red;"></span>
                        </div>



                        <div class="field">
                            <input type="submit" id="submitting" value="SUBMIT"
                                style="margin-left: 290px;border-radius: 10px;">
                        </div>
                    </form>
                </div>
            </div>

    </div>
